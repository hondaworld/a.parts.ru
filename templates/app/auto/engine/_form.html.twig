{% extends 'blocks/block_container.html.twig' %}
{% block content %}
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
    {{ form_row(form.name) }}
    {{ form_row(form.url) }}
    {{ form_row(form.description_tuning) }}
    <button type="submit" class="btn btn-primary">{{ buttonName|default('Сохранить') }}</button>
    <a class="ml-3 btn btn-secondary" href="{{ path('auto.generation.show', {'auto_modelID': autoGeneration.model.id, 'id': autoGeneration.id}) }}">Отменить</a>
    {{ form_end(form) }}
{% endblock %}

