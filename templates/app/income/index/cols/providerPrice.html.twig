{% if 'providerPrice' not in hideCols %}
    <td
            {% if income.status == 1 %}
                class="ajax-modal-html"
                onclick="modalProviderPriceChange(this, {{ income.incomeID }})"
                data-toggle="modal"
                data-target="#modalProviderPrice"
                data-url="{{ path('income.providerPrices', {'id': income.incomeID}) }}"
                style="cursor: pointer;"
            {% endif %}
    >{{ providerPrices[income.providerPriceID]|default(null) ? providerPrices[income.providerPriceID] : 'Возврат клиента' }}
        {% if income.providerPriceBetterPrice|default(null) and income.providerPriceBetterPrice.providerPriceID != income.providerPriceID %}
            <div class="font-weight-bold text-danger">{{ income.providerPriceBetterPrice.providerPriceName }}</div>
        {% endif %}
    </td>
{% endif %}