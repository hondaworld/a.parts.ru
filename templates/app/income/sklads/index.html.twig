<table class="table table-sm table-bordered table-striped">
    <thead>
    <tr class="table-primary">
        <th>Склад</th>
        <th>Нал</th>
        <th>Путь</th>
        <th>Резерв</th>
        <th>Возвр./Спис.</th>
        <th>Мин</th>
        <th>Зак</th>
    </tr>
    </thead>
    <tbody>
    {% for skladID, skladName in sklads %}
        {% if quantityOnSklads[skladID]|default(null) %}
            <tr>
                <td>{{ skladName }}</td>
                <td class="text-center">{{ quantityOnSklads[skladID]['quantityIn'] }}</td>
                <td class="text-center">{{ quantityOnSklads[skladID]['quantityPath'] }}</td>
                <td class="text-center">{{ quantityOnSklads[skladID]['reserve'] }}</td>
                <td class="text-center">{{ quantityOnSklads[skladID]['quantityReturn'] }}</td>
                <td class="text-center">{{ quantityOnSklads[skladID]['quantityMin'] }}</td>
                <td class="text-center">{{ orderedOnSklads[skladID]|default(0) }}</td>
            </tr>
        {% endif %}
    {% endfor %}
    </tbody>
</table>