<script>
    const labels = [
        {% for date in dates %}
        '{{ date|date('d.m') }}',
        {% endfor %}
    ];

    {% for region, regions in profits %}
    {% for opt, opts in regions %}
    const profits_{{ region }}_{{ opt }} =
        {
            'income': [
                {% for date in dates %}
                '{{ profits[region][opt].income.date[date|date('Y-m-d')]|default(0) }}',
                {% endfor %}
            ],
            'profit': [
                {% for date in dates %}
                '{{ profits[region][opt].profit.date[date|date('Y-m-d')]|default(0) }}',
                {% endfor %}
            ],
            'checks': [
                {% for date in dates %}
                '{{ profits[region][opt].checks.date[date|date('Y-m-d')]|default(0) }}',
                {% endfor %}
            ],
            'checksAll': [
                {% for date in dates %}
                '{{ profits[region][opt].checksAll.date[date|date('Y-m-d')]|default(0) }}',
                {% endfor %}
            ]
        };
    {% endfor %}
    {% endfor %}
</script>