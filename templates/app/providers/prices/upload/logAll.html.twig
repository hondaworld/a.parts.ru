{% embed 'tables/table_simple.html.twig' %}

    {% block header_buttons %}{% endblock %}

    {% block table_header %}
        <th>Файл</th>
        <th>Поставщик</th>
        <th>Удалено</th>
        <th>Добавлено</th>
        <th>Дата</th>
        <th>Комментарий</th>
    {% endblock %}

    {% block table_body %}
        {% for log in all %}
            <tr>
                <td>{{ log.price }}</td>
                <td>{{ log.providerName }}</td>
                <td>{{ log.countDelete }}</td>
                <td>{{ log.countInsert }}</td>
                <td>{{ log.dateofadded|date('d.m.Y H:i:s') }}</td>
                <td>{{ log.comment|raw }}</td>
            </tr>
        {% endfor %}
    {% endblock %}
{% endembed %}